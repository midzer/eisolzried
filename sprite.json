{
    "mode": {
      "stack": {
        "dest": "icons",
        "sprite": "sprite.svg"
      }
    },
    "shape": {
      "transform": [
        {
          "svgo": {
            "plugins": [
              "preset-default"
            ]
          }
        }
      ]
    },
    "svg": {
      "xmlDeclaration": false,
      "doctypeDeclaration": false,
      "namespaceIDs": false,
      "namespaceClassnames": false,
      "dimensionAttributes": false
    }
}
